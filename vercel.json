{
  "version": 2,
  "builds": [
    {"src": "backend/index.js",
     "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
     "use": "@vercel/static-build",
     "config": {"distDir": "dist"}}
  ],
  "routes": [
    {"src": "/api/(.*)", "dest": "/backend/index.js",
    "headers": {
        "Access-Control-Allow-Origin": "*"
      }
  },

    {"src": "/(.*)", "dest": "/client/dist/$1"}
  ]
}